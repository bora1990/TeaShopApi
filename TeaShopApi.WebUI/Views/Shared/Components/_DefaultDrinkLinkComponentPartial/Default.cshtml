@model List<TeaShopApi.WebUI.Dtos.DrinkDtos.ResultDrinkDto>

<div class="services_section layout_padding">
    <div class="container">
        <h1 class="services_taital">İçercek Servisleri</h1>
        <p class="services_text">Uzun yılların tecrübesiyle hizmete devam </p>
        <div class="services_section_2 layout_padding">
            <div class="row">
                 @foreach(var item in Model)
                    {
                <div class="col-md-6">
                   
                                
                    <div class="box_main">
                        <div class="left_main">
                            <div class="cup_img_1"><img src="@item.drinkImageUrl" width="100%" height="100%"></div>
                        </div>
                        <div class="middle_main">
                            <div class="border_10 active"></div>
                        </div>
                        <div class="right_main">
                            <h6 class="milk_text">@item.drinkName</h6>
                            <h1 class="price_text">@item.drinkPrice</h1>
                        </div>
                    </div>
                   
                </div>
                }
            </div>
        </div>
        <div class="see_bt"><a href="#">Daha Fazlası</a></div>
    </div>
</div>